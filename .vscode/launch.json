    "version": "0.2.0",
    "configurations": [
        {
            "name": "调试 frpcgui GUI (需要管理员)",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/cmd/frpcgui",
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            "showLog": false
        },
        {
            "name": "调试 frpcgui (指定配置文件)",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/cmd/frpcgui",
            "cwd": "${workspaceFolder}",
            "args": [
                "-c",
                "${workspaceFolder}/test.toml"
            ],
            "console": "integratedTerminal",
            "showLog": true,
            "buildFlags": "-gcflags='all=-N -l' -ldflags='-H windowsgui'"
        },
        {
            "name": "调试当前测试文件",
            "type": "go",
            "request": "launch",
            "mode": "test",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "调试当前测试函数",
            "type": "go",
            "request": "launch",
            "mode": "test",
            "program": "${file}",
            "args": [
                "-test.run",
                "^${selectedText}$"
            ],
            "console": "integratedTerminal"
        }
    ]
}